{
  "name": "perfcopilot",
  "displayName": "PerfCopilot",
  "description": "AI-powered JavaScript function performance analyzer for VS Code",
  "version": "0.0.1",
  "publisher": "ArnoldoM23",
  "private": false,
  "license": "MIT",
  "icon": "assets/logo.png",
  "keywords": [
    "javascript",
    "performance",
    "benchmark",
    "optimization",
    "function"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ArnoldoM23/PerfCopilot.git"
  },
  "homepage": "https://github.com/ArnoldoM23/PerfCopilot",
  "bugs": {
    "url": "https://github.com/ArnoldoM23/PerfCopilot/issues"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:perfcopilot.analyzeFunction"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "perfcopilot.analyzeFunction",
        "title": "Analyze Function Performance"
      },
      {
        "command": "perfcopilot.showLogs",
        "title": "PerfCopilot: Show Logs"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "perfcopilot.analyzeFunction",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "@types/benchmark": "^2.1.5"
  },
  "dependencies": {
    "benny": "^3.7.1"
  }
}
