{
  "name": "perfcopilot",
  "displayName": "PerfCopilot",
  "description": "AI-powered JavaScript function performance analyzer for VS Code",
  "version": "0.0.1",
  "publisher": "ArnoldoM23",
  "private": false,
  "license": "MIT",
  "icon": "assets/logo.png",
  "keywords": [
    "javascript",
    "performance",
    "benchmark",
    "optimization",
    "function"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ArnoldoM23/PerfCopilot.git"
  },
  "homepage": "https://github.com/ArnoldoM23/PerfCopilot",
  "bugs": {
    "url": "https://github.com/ArnoldoM23/PerfCopilot/issues"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:perfcopilot.analyzeFunction"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "perfcopilot.analyzeFunction",
        "title": "Analyze Function Performance"
      },
      {
        "command": "perfcopilot.showLogs",
        "title": "PerfCopilot: Show Logs"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "perfcopilot.analyzeFunction",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "18.x",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vscode/test-utils": "^0.3.5",
    "@vscode/vsce": "^2.24.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "sinon": "^20.0.0",
    "ts-jest": "^29.3.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "benny": "^3.7.1"
  },
  "extensionDependencies": [
    "GitHub.copilot"
  ]
}
